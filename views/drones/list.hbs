
<h2>Available drones</h2>



<div>
{{#each allDrones}}
<h4>Nombre:</h4> <h5>{{this.name}}</h5>
<h4>Propellers:</h4>  <h5>{{this.propellers}}</h5>
<h4>Speed:</h4>  <h5>{{this.maxSpeed}}</h5>

<a href="/drones/{{this._id}}/edit">editar un dron</a>
    <form action="/drones/{{this._id}}/delete" method="post">
    <button>Borrar</button>
    </form>
{{/each}}


</div>